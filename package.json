{
  "name": "angular1And2",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "npm run tsd-install && npm run tsd-link",
    "tsd-install": "tsd install --config src/tsd.json",
    "tsd-link": "tsd link --config src/tsd.json",
    "tsc": "tsc -p src -w",
    "start": "live-server --open=src"
  },
  "dependencies": {
    "angular": "^1.4.7",
    "angular-ui-router": "^0.2.15",
    "angular2": "2.0.0-alpha.40",
    "systemjs": "0.19.2"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-inject": "^3.0.0",
    "gulp-jshint": "^1.11.2",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.4.1",
    "jshint-stylish": "^2.0.1",
    "live-server": "^0.8.1",
    "rimraf": "^2.4.3",
    "tsd": "^0.6.5",
    "typescript": "^1.6.2"
  }
}
